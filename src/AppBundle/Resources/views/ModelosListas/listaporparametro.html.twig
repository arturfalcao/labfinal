<script>
    var rowIds = [];
    var selectedRows = [];
    var _lastSelectedRow = 0;
    var _ParaData = [];
    var _oldoriginalvalue = 0;
    var _parametro = 0;
    var _amostras = "";
    var _metodo ="";
    var condi;

    function getlist(){
        _amostras = "";
        for (var i = 0; i < rowIds.length; i++)
        {
            _amostras = rowIds[i] + "," + _amostras;
        }
        if(_amostras!= "") {
            if (_amostras.charAt(_amostras.length - 1) == ',')
                _amostras = _amostras.slice(0, _amostras.length - 1);
            $.ajax({
                type: "POST",
                url: '{{ path('GenerateworklistbyParameter') }}',
                data: {amostras: _amostras, parametro: _parametro, metodo: _metodo},
                success: function (data) {
                    if (data != "") {
                        data = data.replace(/['"]+/g, '');
                        location.reload();
                        window.open("/app_dev.php/getpdflista/" + data, "_blank");
                    } else {

                    }
                }
            });
        }
        else{
            toastr.info("Selecione pelo menos um parâmetro da grelha");
        }
    }

    function getdata(){

        if( $('.parametro_ent_resultados').select2('data') != null){
            _parametro = $('.parametro_ent_resultados').select2('data').id;
            if( $('.parametro_metodo').select2('data') != null)
                _metodo = $('.parametro_metodo').select2('data').id;
            else
                _metodo = 0;
            $.ajax({
                type: "POST",
                url: '{{path('GetparameterbysampleForParameter')}}',
                data: _parametro + ',' + _metodo,
                success: function(data) {
                    if(data != "null"){
                        $("#lista_para_grid").bootgrid("destroy");
                        var grid_para = $("#lista_para_grid").bootgrid({
                            navigation:0,
                            selection: true,
                            multiSelect: true,
                            rowSelect: true,
                            keepSelection: true
                        }).on("selected.rs.jquery.bootgrid", function(e, rows)
                        {
                            for (var i = 0; i < rows.length; i++)
                            {
                                rowIds.push(Number(rows[i].fn_id_amostra));
                            }
                        }).on("deselected.rs.jquery.bootgrid", function(e, rows)
                        {
                            for (var i = 0; i < rows.length; i++)
                            {
                                $.each(rowIds, function( index, value ) {
                                    if(value == rows[i].fn_id_amostra){
                                        rowIds.splice(index,1);
                                    }
                                });


                            }
                        });
                        var obj = $.parseJSON(data);

                        if (obj.length != 0) {
                            grid_para.bootgrid("append",obj.data );
                        } else {

                        }
                    }else{
                        $("#lista_para_grid").bootgrid("destroy");
                        toastr.info("Não foram encontrados resultados para o seu pedido.")
                    }
                }
            });
        }else{


        }


    }

    $(document).ready(function() {

        $(".parametro_ent_resultados").select2({
            allowClear: true,
            placeholder: "Selecione um Pârametro"
        }).on("change", function (e) {
            GetAllMetodo(e.val);
        });

        $(".parametro_metodo").select2({
            allowClear: true,
            placeholder: "Selecione um Método"
        });

        function GetAllMetodo(_idpara){
                $.ajax({
                    type: "POST",
                    url: "{{ path('GetMetodoPorParametro') }}",
                    data: {data: _idpara},
                    success: function (data) {
                        $("select.parametro_metodo").html('');
                        data = JSON.parse(data);
                        $('select.parametro_metodo').append("<option></option>");
                       $.each(data, function (index, value) {
                            $('select.parametro_metodo').append(
                                    '<option value="' + value.fnId + '">' + value.ftDescricao + '</option>'
                            );
                        });


                    }
                });

        }



    });

</script>

<style>
    .circulo{
        margin-left: 250px!important;
    }
</style>

</body>
</html>