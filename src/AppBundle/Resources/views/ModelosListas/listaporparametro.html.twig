                    {% endblock %}

                    {% block list_table %}
                        <table id="lista_para_grid" class="table table-condensed table-hover table-striped">
                            <thead>
                            <tr>
                                <th data-column-id="fn_id_amostra" data-type="numeric" data-identifier="true">ID Amostra</th>
                                <th data-column-id="ft_descricao">Parâmetro</th>
                                <th data-column-id="ft_id_estado">Estado</th>
                                <th data-column-id="metodo" >Método</th>
                                <th data-column-id="tecnica">Técnica</th>
